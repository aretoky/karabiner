{
   "title": "Capslockキー単独でGyazo起動",
   "rules": [
     {
       "description": "Capslockキー単独でGyazo起動",
       "manipulators": [
         {
           "type": "basic",
           "from": {
             "key_code": "caps_lock"
           },
           "to_if_alone": [
             {
               "shell_command": "open -a 'Gyazo.app'"
             }
           ]
         }
       ]
     }
   ]
 }
