{
  "title": "Change EISU + *",
  "rules": [
    {
      "description": "Change EISU + *",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "japanese_eisuu"
          },
          "to": [
            {
              "set_variable": {
                "name": "eisuu",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "eisuu",
                "value": 0
              }
            }
          ],
          "to_if_alone": [
            { 
              "key_code": "japanese_eisuu"
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "h",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "j",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "down_arrow"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "k",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "up_arrow"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "l",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "right_arrow"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "e",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "escape"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "description": "メタキー+tでtaskchute cloud起動",
          "type": "basic",
          "from": { "key_code": "t" },
          "to": [
            {
              "shell_command": "open -a 'TaskChute Cloud.app'"
            },
            { "set_variable": { "name": "press_application_select_key", "value": 0 } }
          ],
          "conditions": [
            { "type": "variable_if", "name": "press_application_select_key", "value": 1 }
          ]
        },
        {
          "description": "メタキー+sでslack起動",
          "type": "basic",
          "from": { "key_code": "s" },
          "to": [
            {
              "shell_command": "open -a 'Slack.app'"
            },
            { "set_variable": { "name": "press_application_select_key", "value": 0 } }
          ],
          "conditions": [
            { "type": "variable_if", "name": "press_application_select_key", "value": 1 }
          ]
        },
        {
          "description": "メタキー+bでbiscuit起動",
          "type": "basic",
          "from": { "key_code": "b" },
          "to": [
            {
              "shell_command": "open -a 'Biscuit.app'"
            },
            { "set_variable": { "name": "press_application_select_key", "value": 0 } }
          ],
          "conditions": [
            { "type": "variable_if", "name": "press_application_select_key", "value": 1 }
          ]
        },
        {
          "description": "メタキー+eでiterm起動",
          "type": "basic",
          "from": { "key_code": "e" },
          "to": [
            {
              "shell_command": "open -a 'iTerm.app'"
            },
            { "set_variable": { "name": "press_application_select_key", "value": 0 } }
          ],
          "conditions": [
            { "type": "variable_if", "name": "press_application_select_key", "value": 1 }
          ]
        },
        {
          "description": "メタキー+iでintelliJ起動",
          "type": "basic",
          "from": { "key_code": "i" },
          "to": [
            {
              "shell_command": "open -a 'IntelliJ IDEA.app'"
            },
            { "set_variable": { "name": "press_application_select_key", "value": 0 } }
          ],
          "conditions": [
            { "type": "variable_if", "name": "press_application_select_key", "value": 1 }
          ]
        },
        {
          "description": "メタキー+gでchrome起動",
          "type": "basic",
          "from": { "key_code": "g" },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            },
            { "set_variable": { "name": "press_application_select_key", "value": 0 } }
          ],
          "conditions": [
            { "type": "variable_if", "name": "press_application_select_key", "value": 1 }
          ]
        },
        {
          "description": "メタキー+aでactive monitor起動",
          "type": "basic",
          "from": { "key_code": "a" },
          "to": [
            {
              "shell_command": "open -a 'Activity Monitor.app'"
            },
            { "set_variable": { "name": "press_application_select_key", "value": 0 } }
          ],
          "conditions": [
            { "type": "variable_if", "name": "press_application_select_key", "value": 1 }
          ]
        },
        {
          "description": "EISUU+fでメタキー発動",
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": { "name": "press_application_select_key", "value": 1 }
            }
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              { "set_variable": { "name": "press_application_select_key", "value": 0 } }
            ],
            "to_if_canceled": [
              { "set_variable": { "name": "press_application_select_key", "value": 0 } }
            ]
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "press_application_select_key",
              "value": 0
            },
            {
              "type": "variable_if",
              "name": "eisuu",
              "value": 1
            }
          ]
        },
	    {
          "type": "basic",
          "from": { 
            "key_code": "e",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "escape"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "d",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "delete_or_backspace"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
	    {
          "type": "basic",
          "from": { 
            "key_code": "n",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "i",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "page_up"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": { 
            "key_code": "o",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "page_down"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "description": "alfredのclipboard",
          "type": "basic",
          "from": { 
            "key_code": "u",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "u",
              "modifiers": ["control", "option"]
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "description": "iterm",
          "type": "basic",
          "from": { 
            "key_code": "q",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "spacebar",
              "modifiers": ["control"]
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        },
        {
          "description": "セミコロンをEISU+;で。Enterにバインドした時用",
          "type": "basic",
          "from": { 
            "key_code": "semicolon",
            "modifiers":{
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            { 
              "key_code": "semicolon"
            }
          ],
          "conditions": [
            { 
              "type": "variable_if", 
              "name": "eisuu", 
              "value": 1
            }
          ]
        }
      ]
    }
  ]
}
